{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*).(js|css|png|jpg|jpeg|gif|ico|svg)",
        "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
        "dest": "/build/$1.$2"
      },
      {
        "src": "/(.*)",
        "headers": { "Cache-Control": "public, max-age=0, must-revalidate" },
        "dest": "index.html"
      }
    ]
  }
  